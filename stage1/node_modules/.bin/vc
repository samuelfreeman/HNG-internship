#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sam/Documents/HNG-internship/stage1/node_modules/.pnpm/vercel@34.2.8/node_modules/vercel/dist/node_modules:/home/sam/Documents/HNG-internship/stage1/node_modules/.pnpm/vercel@34.2.8/node_modules/vercel/node_modules:/home/sam/Documents/HNG-internship/stage1/node_modules/.pnpm/vercel@34.2.8/node_modules:/home/sam/Documents/HNG-internship/stage1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sam/Documents/HNG-internship/stage1/node_modules/.pnpm/vercel@34.2.8/node_modules/vercel/dist/node_modules:/home/sam/Documents/HNG-internship/stage1/node_modules/.pnpm/vercel@34.2.8/node_modules/vercel/node_modules:/home/sam/Documents/HNG-internship/stage1/node_modules/.pnpm/vercel@34.2.8/node_modules:/home/sam/Documents/HNG-internship/stage1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vercel/dist/index.js" "$@"
else
  exec node  "$basedir/../vercel/dist/index.js" "$@"
fi
